# Last Updated: 2026-02-11 [Stash Override v1.46]
# 移植自 PC 端 Mihomo_Override.js v1.79
# 引用链接: https://raw.githubusercontent.com/TamperAcc/Clash/main/Stash_Override.stoverride
# 加速链接: https://cdn.jsdelivr.net/gh/TamperAcc/Clash@main/Stash_Override.stoverride
# Stash 元数据
name: "Stash Override v1.46"
desc: "v1.46 | 纯净版: 同步学术网站分流 (Scholar)"
icon: https://cdn.jsdelivr.net/gh/Orz-3/mini@master/Color/Stash.png
# ==========================================
# 核心网络设置 (TCP/DNS/MitM/HTTP)
# ==========================================
# 1. TCP 并发 & 基础优化
tcp-concurrent: true
keep-alive-interval: 30
ipv6: false        # 🚫 强制关闭 IPv6 协议栈 (解决 no route to host 报错)
prefer-ipv6: false # 📱 iOS 5G 优化: 强制关闭 IPv6 防止 Google/YouTube 延迟过高
allow-lan: false   # 🛡️ 安全加固: 手机端默认关闭局域网共享，防止被扫描
bind-address: "*"  # 配合 allow-lan: false，此项仅在本机生效
# 2. GeoData 内存优化模式
geodata-mode: true
geodata-loader: memconservative
geox-url:
  geoip: "https://testingcf.jsdelivr.net/gh/MetaCubeX/meta-rules-dat@release/geoip.metadb"
  geosite: "https://testingcf.jsdelivr.net/gh/MetaCubeX/meta-rules-dat@release/geosite.dat"
  mmdb: "https://testingcf.jsdelivr.net/gh/MetaCubeX/meta-rules-dat@release/geoip.dat"
# 3. 模拟指纹
global-client-fingerprint: chrome
# 4. DNS 设置
dns:
  enable: true
  ipv6: false
  enhanced-mode: fake-ip
  fake-ip-range: 198.18.0.1/16
  respect-rules: true
  fake-ip-filter:
    - "*.lan"
    - "*.local"
    - "stun.*"
    - "time.*.apple.com"
    - "time1.cloud.tencent.com"
    - "*.bambulab.com"
    - "*.bambulab.cn"
    - "+.stun.*.*"
    - "+.stun.*"
    - "+.nintendo.net"
    - "+.playstation.net"
    - "+.xboxlive.com"
    - "+.msftconnecttest.com"
    - "+.msftncsi.com"
    # ⬇️ 同步 Mihomo: 补充 NTP 和 协议相关过滤
    - "+.ntp.org"
    - "+.pool.ntp.org"
    - "+.stun.protocol.org"
    - "time.*.com"
    - "time.*.gov"
    - "time.*.edu.cn"
  
  nameserver-policy:
    '+.apple.com': 'system'
    '+.icloud.com': 'system'
    '+.mzstatic.com': 'system'
    '+.id.apple.com': 'system'
    '+.itunes.apple.com': 'system'
    '+.apps.apple.com': 'system'
    '+.crashlytics.com': 'system'
    'geosite:cn': '119.29.29.29'
    '+.bambulab.cn': '119.29.29.29'
    # ⬇️ 同步 Mihomo: 补充 Bambu Global 域名
    '+.bambulab.com': '119.29.29.29'
  
  nameserver: #!replace
    - quic://dns.alidns.com:853 # 🚀 阿里 DoQ: 0-RTT 连接，5G/WiFi 切换不断流
    - https://doh.pub/dns-query # 腾讯 DoH (iOS 推荐: 抗劫持能力最强，解决节点超时)
    - 119.29.29.29 # 腾讯 UDP (兜底备用)
    # - system # ❌ 移除系统 DNS: iOS 上运营商DNS经常污染节点域名导致连不上
  
  # ✅ 新增 Fallback: 采用 OpenDNS TCP (更稳) + Cloudflare DoH (去污染)，移除 Google TCP
  fallback:
    - tcp://208.67.222.222:443
    - https://1.1.1.1/dns-query
  
  fallback-filter:
    geoip: true
    geoip-code: CN
    ipcidr:
      - 240.0.0.0/4

# 4.1 Sniffer 嗅探 (iOS 省电优化)
# ⚠️ 注意: 除非必须，否则建议关闭或仅特定协议开启，以降低 CPU 唤醒
sniffer:
  enable: false  # 🔋 省电优先：默认关闭嗅探，减少发热。仅当需要根据 SNI 分流纯 IP 请求时才开启。
  parse-pure-ip: true
  override-destination: true
  sniff:
    HTTP:
      ports: [80, 8080, 8880]
      override-destination: true
    TLS:
      ports: [443, 8443]
# 5. MitM (HTTPS 解密)
mitm:
  enable: true
  append-system-trust: true
  hostname:
    - "*.google.cn"
    - "*.google.com.cn"
    - "www.google.com"
# 6. HTTP 引擎
http:
  force-http-engine-hosts:
    - "*.google.cn"
    - "*.google.com.cn"
rewrite:
  - match: ^https?://(www\.)?google\.cn
    replace: https://www.google.com
    type: 302
  - match: ^https?://(www\.)?google\.com\.cn
    replace: https://www.google.com
    type: 302
# ==========================================
# 策略组 (Proxy Groups) - 核心逻辑移植
# ==========================================
proxy-groups: #!replace
  - name: 自动选择
    type: url-test
    icon: https://cdn.jsdelivr.net/gh/Orz-3/mini@master/Color/Urltest.png
    include-all: true
    # 策略优化: 移除香港节点的排除 ("香港|hongkong|hk|HK|Hong|Kong|")，大幅降低通用浏览延迟; 排除 IEPL 以节省专线流量
    filter: "(?i)^(?!.*(IEPL|立陶宛|Lithuania|LT|俄罗斯|Russia|RU|流量|到期|重置|官网|剩余|套餐|expire|traffic|reset|群组|频道|@|联系|网站|入群|关注|反馈|更新)).*"
    url: "https://www.gstatic.com/generate_204"
    interval: 600   # 🔋 极致省电: 延长测速间隔至 10分钟，减少基带唤醒
    tolerance: 50
    unified-delay: true
    lazy: true

  - name: 🔍 搜索与开发
    type: url-test
    icon: https://cdn.jsdelivr.net/gh/Orz-3/mini@master/Color/Google.png
    include-all: true
    # 逻辑: 只要不是流量/到期等关键词，全部保留 (含 HK/SG)
    filter: "(?i)^(?!.*(立陶宛|Lithuania|LT|俄罗斯|Russia|RU|流量|到期|重置|官网|剩余|套餐|expire|traffic|reset|群组|频道|@|联系|网站|入群|关注|反馈|更新)).*"
    url: "http://www.gstatic.com/generate_204"
    interval: 600   # 🔋 极致省电
    tolerance: 50
    unified-delay: false # ⚡ 极速模式
    lazy: true

  - name: 🤖 生成式 AI
    type: url-test
    icon: https://cdn.jsdelivr.net/gh/Orz-3/mini@master/Color/OpenAI.png
    include-all: true
    # 逻辑: 剔除 HK/HongKong/香港/CN/China/澳门/Macau 以及 俄罗斯/Russia/RU/立陶宛/Lithuania/朝鲜/古巴
    filter: "(?i)^(?!.*(立陶宛|Lithuania|LT|俄罗斯|俄国|Russia|RU|Russia|香港|hongkong|hk|HK|Hong|Kong|澳门|Macau|朝鲜|Korea|KP|古巴|Cuba|CU|圣何塞|流量|到期|重置|官网|剩余|套餐|expire|traffic|reset|群组|频道|@|联系|网站|入群|关注|反馈|更新)).*"
    url: "https://www.youtube.com" # 优化: 升级为 YouTube 纯净度检测 (防送中)
    expected-status: 200           # 优化: 强制要求 200 OK，同步 PC 端逻辑
    interval: 600   # 🔋 极致省电
    tolerance: 100
    unified-delay: false # ⚡ 极速模式
    lazy: true

  - name: Telegram
    type: url-test
    icon: https://raw.githubusercontent.com/Orz-3/mini/master/Color/Telegram.png
    include-all: true
    # 排除立陶宛和俄罗斯
    filter: "(?i)^(?!.*(俄罗斯|Russia|RU|立陶宛|Lithuania|LT|流量|到期|重置|官网|剩余|套餐|expire|traffic|reset|群组|频道|联系|网站|入群|专线)).*"
    url: "https://api.telegram.org"
    interval: 600   # 🔋 极致省电
    tolerance: 100
    unified-delay: true

  - name: 国内
    type: select
    icon: https://cdn.jsdelivr.net/gh/Orz-3/mini@master/Color/CN.png
    proxies:
      - DIRECT
      - 自动选择
  
  - name: Google Search
    type: select
    icon: https://cdn.jsdelivr.net/gh/Orz-3/mini@master/Color/Google.png
    proxies:
      - 🔍 搜索与开发
      - 🤖 生成式 AI
      - 自动选择

  - name: YouTube 4K
    type: select
    icon: https://cdn.jsdelivr.net/gh/Orz-3/mini@master/Color/YouTube.png
    proxies:
      - 自动选择
      - 🔍 搜索与开发
      
  - name: 国外通用
    type: select
    icon: https://cdn.jsdelivr.net/gh/Orz-3/mini@master/Color/Global.png
    proxies:
      - 自动选择
      - 🔍 搜索与开发
# ==========================================
# 规则集合 (Rule Providers) - ❌ 已废弃 (全量迁移至 Geosite)
