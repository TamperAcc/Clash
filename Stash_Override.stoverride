# Last Updated: 2026-02-21 [Stash Override v1.80]
# 移植自 PC 端 Mihomo_Override.js v1.89
# 引用链接: https://raw.githubusercontent.com/TamperAcc/Clash/main/Stash_Override.stoverride
# 加速链接: https://cdn.jsdelivr.net/gh/TamperAcc/Clash@main/Stash_Override.stoverride
# Stash 元数据
name: "Stash Override v1.80"
desc: "v1.80 | 极致省电优化: 移除 MitM/HTTP，高频规则置顶，同步 AI 规则"
icon: https://cdn.jsdelivr.net/gh/Orz-3/mini@master/Color/Stash.png
# ==========================================
# 核心网络设置 (TCP/DNS/MitM/HTTP)
# ==========================================
# 1. TCP 并发 & 基础优化
tcp-concurrent: true
keep-alive-interval: 30
ipv6: false        # 🚫 强制关闭 IPv6 协议栈 (解决 no route to host 报错)
prefer-ipv6: false # 📱 iOS 5G 优化: 强制关闭 IPv6 防止 Google/YouTube 延迟过高
allow-lan: false   # 🛡️ 安全加固: 手机端默认关闭局域网共享，防止被扫描
bind-address: "*"  # 配合 allow-lan: false，此项仅在本机生效
# 2. GeoData 内存优化模式
geodata-mode: true
geodata-loader: memconservative
geox-url:
  geoip: "https://testingcf.jsdelivr.net/gh/MetaCubeX/meta-rules-dat@release/geoip.metadb"
  geosite: "https://testingcf.jsdelivr.net/gh/MetaCubeX/meta-rules-dat@release/geosite.dat"
  mmdb: "https://testingcf.jsdelivr.net/gh/MetaCubeX/meta-rules-dat@release/geoip.dat"
# 3. 模拟指纹
global-client-fingerprint: chrome

# 4. DNS 设置
dns:
  enable: true
  ipv6: false
  enhanced-mode: fake-ip
  fake-ip-range: 198.18.0.1/16
  respect-rules: true
  # 🚀 Bootstrap DNS: 必须配置，否则无法解析 DoH 域名
  default-nameserver:
    - 223.5.5.5
    - 1.12.12.12
  fake-ip-filter:
    - "*.lan"
    - "*.local"
    - "stun.*"
    - "time.*.apple.com"
    - "time1.cloud.tencent.com"
    - "*.bambulab.com"
    - "*.bambulab.cn"
    - "*.stun.*.*"
    - "*.stun.*"
    - "+.nintendo.net"
    - "+.playstation.net"
    - "+.xboxlive.com"
    - "+.msftconnecttest.com"
    - "+.msftncsi.com"
    # ⬇️ 同步 Mihomo: 补充 NTP 和 协议相关过滤
    - "+.ntp.org"
    - "+.pool.ntp.org"
    - "+.stun.protocol.org"
    - "time.*.com"
    - "time.*.gov"
    - "time.*.edu.cn"

  # 🚀 Main Nameservers (DoH 防劫持)
  nameserver:
    # 默认走阿里/腾讯 DoH (IP 直连，极速且加密)
    - https://223.5.5.5/dns-query
    - https://1.12.12.12/dns-query
    
  # 🛡️ 策略分流 (解决 context deadline exceeded 报错)
  nameserver-policy:
    # Apple/iCloud 极其敏感，强制走 System (iOS系统原生DNS) 以消除报错
    # 优化: 使用 geosite:apple 统一接管所有 Apple 服务 DNS，确保 CDN 速度和推送稳定
    'geosite:apple': 'system'
    '+.crashlytics.com': 'system'
    # Bambu Lab 也建议走 System 确保局域网发现
    '+.bambulab.cn': 'system'
    '+.bambulab.com': 'system'
    # 国内 Geosite 走腾讯 UDP (快)
    'geosite:cn': '119.29.29.29'

  fallback:
    # 移动网络下，Fallback 仅作为 nameserver 彻底挂掉的冷备
    - https://1.12.12.12/dns-query
    - https://223.5.5.5/dns-query
  
  fallback-filter:
    geoip: true
    geoip-code: CN
    ipcidr:
      - 240.0.0.0/4

# 4.1 Sniffer 嗅探 (iOS 省电优化)
# ⚠️ 注意: 除非必须，否则建议关闭或仅特定协议开启，以降低 CPU 唤醒
sniffer:
  enable: false  # 🔋 省电优先。若需精确分流由纯 IP 访问的域名(如 Spotify/Netflix)，可在此开启
  parse-pure-ip: true
  override-destination: true
  sniff:
    HTTP:
      ports: [80, 8080, 8880]
      override-destination: true
    TLS:
      ports: [443, 8443]
    QUIC: # v3.3.0+ 支持 HTTP/3 嗅探
      ports: [443, 8443]

# ==========================================
# 远程代理集 (Proxy Providers)
# ==========================================
proxy-providers: #!replace
  组合机场:
    type: http
    url: "https://sub.store/download/collection/%E7%BB%84%E5%90%88%E6%9C%BA%E5%9C%BA?target=ClashMeta"
    interval: 86400
    path: ./proxies/sub_store_collection.yaml
    health-check:
      enable: true
      interval: 600
      url: http://www.gstatic.com/generate_204

# ==========================================
# 策略组 (Proxy Groups) - 核心逻辑移植
# ==========================================
proxy-groups: #!replace
  - name: 自动选择
    type: url-test
    icon: https://cdn.jsdelivr.net/gh/Orz-3/mini@master/Color/Urltest.png
    include-all: true
    use:
      - 组合机场
    # 策略优化: 移除香港节点排除，大幅降低通用延迟; 排除 IEPL 以节省专线
    filter: "(?i)^(?!.*(IEPL|立陶宛|Lithuania|LT|俄罗斯|Russia|RU|流量|到期|重置|官网|剩余|套餐|expire|traffic|reset|群组|频道|@|联系|网站|入群|关注|反馈|更新)).*"
    url: "https://www.gstatic.com/generate_204"
    interval: 600   # 🔋 极致省电
    tolerance: 100
    unified-delay: true
    lazy: true

  - name: Gemini
    type: url-test
    icon: https://cdn.jsdelivr.net/gh/Orz-3/mini@master/Color/Google.png
    include-all: true
    use:
      - 组合机场
    # 逻辑: 剔除 HK/CN/JP/MO/LT/RU/KP/CU 用于 AI 防风控 (加上韩国/KR)
    filter: "(?i)^(?!.*(立陶宛|Lithuania|LT|俄罗斯|俄国|Russia|RU|香港|hongkong|hk|HK|Hong|Kong|澳门|Macau|MO|日本|Japan|JP|韩国|Korea|KR|朝鲜|KP|古巴|Cuba|CU|圣何塞|流量|到期|重置|官网|剩余|套餐|expire|traffic|reset|群组|频道|@|联系|网站)).*"
    url: "https://gemini.google.com" 
    expected-status: 200
    interval: 630
    tolerance: 100
    unified-delay: false
    lazy: true

  - name: ChatGPT
    type: url-test
    icon: https://cdn.jsdelivr.net/gh/Orz-3/mini@master/Color/OpenAI.png
    include-all: true
    use:
      - 组合机场
    # 逻辑: 严格风控，剔除 HK/CN/MO/RU/KP/CU/IR/SY
    filter: "(?i)^(?!.*(立陶宛|Lithuania|LT|俄罗斯|俄国|Russia|RU|香港|hongkong|hk|HK|Hong|Kong|澳门|Macau|MO|朝鲜|KP|古巴|Cuba|CU|伊朗|Iran|SY|叙利亚|流量|到期|重置|官网|剩余|套餐|expire|traffic|reset|群组|频道|@|联系|网站)).*"
    url: "https://chat.openai.com" 
    expected-status: 200
    interval: 660
    tolerance: 100
    unified-delay: false
    lazy: true

  - name: 国内
    type: select
    icon: https://cdn.jsdelivr.net/gh/Orz-3/mini@master/Color/CN.png
    proxies:
      - DIRECT
      - 自动选择

# ==========================================
# 规则集合 (Rule Providers) - ❌ 已废弃 (全量迁移至 Geosite)
# ==========================================
# rule-providers: #!replace

# ==========================================
# 规则 (Rules)
# ==========================================
rules: #!replace

  # 🛡️ 强制关闭 QUIC (v3.3.0+ 新语法: PROTOCOL 匹配更精准，覆盖所有端口)
  - PROTOCOL,QUIC,REJECT
  
  # 🔋 [电量优化] 高频背景流量置顶：iOS 会在后台疯狂请求 Apple 服务和局域网
  # 提前拦截这些流量，可避免遍历后续几十条规则，大幅降低 CPU 唤醒和匹配开销
  - GEOSITE,apple,DIRECT
  - DOMAIN-SUFFIX,lan,DIRECT
  - DOMAIN-SUFFIX,local,DIRECT
  - DOMAIN-SUFFIX,home.arpa,DIRECT
  
  # === 0. AI 核心规则 (Geosite 统一匹配) ===
  # 🚀 AI 规则优化: 使用 Geosite 替代 Rule Set (内存极省)
  - GEOSITE,openai,ChatGPT
  - GEOSITE,category-ai-chat-!cn,Gemini
  # - GEOSITE,google,Gemini (STUN 修复后，无需在此处强制前提，恢复原位即可减少误伤) - [v1.74 回退]

  - GEOIP,PRIVATE,DIRECT,no-resolve
  
  # === 2. 广告拦截与隐私保护 (Geosite 内存优化) ===
  # ♻️ 替换 RULE-SET,reject 为 Geosite,category-ads-all
  - GEOSITE,category-ads-all,REJECT
  # (移除 redundant 显式域名，交由 Geosite 处理)
  
  # === 3. 进程匹配 (iOS/macOS) ===
  # 🧹 清理：移除了大量 Windows .exe 规则，减少 iOS 匹配开销
  # ⚠️ 注意: 当前文件仅供 iOS 使用，已移除 macOS 专属的 PROCESS-NAME 规则
  - DOMAIN-SUFFIX,bambulab.com,DIRECT
  - DOMAIN-SUFFIX,bambulab.cn,DIRECT
  - DOMAIN-SUFFIX,bambulab.co,DIRECT

  # 基础连通性修正 (IPv6)
  - DOMAIN-SUFFIX,yfjc.xyz,DIRECT
  - DOMAIN-SUFFIX,1huanlesap02.top,DIRECT
  
  # 测速服务
  - GEOSITE,ookla-speedtest,DIRECT
  - DOMAIN-SUFFIX,fast.com,自动选择
  - DOMAIN-SUFFIX,startspoint.com,自动选择
  
  # 优化: 移除冗余 CDN 及国内域名列表, 使用 Geosite:cn 统一代理
  # - DOMAIN-SUFFIX,cdn.jsdelivr.net,DIRECT (已移除，防JSdelivr污染)
  
  - DOMAIN-SUFFIX,stackoverflow.com,自动选择
  - DOMAIN-SUFFIX,stackexchange.com,自动选择
  # 社交
  - GEOSITE,telegram,自动选择
  - GEOIP,telegram,自动选择
  
  - DOMAIN-SUFFIX,npmjs.com,自动选择
  - DOMAIN-SUFFIX,pypi.org,自动选择
  - DOMAIN-SUFFIX,docker.com,自动选择
  - DOMAIN-SUFFIX,docker.io,自动选择
  
  - DST-PORT,123,DIRECT
  
  # === AI & 搜索 精细化分流 ===
  
  # 1. Copilot VScode 插件 -> 自动选择 (求快，允许 HK/SG)
  # 修复 Bing 重定向循环：国内版 Bing 强制直连
  - DOMAIN,cn.bing.com,DIRECT
  - DOMAIN-SUFFIX,bing.com,自动选择
  - DOMAIN-SUFFIX,copilot.microsoft.com,自动选择
  
  # 3. Google -> Gemini (用户指定) - [v1.74 恢复原位]
  # 强化 Gemini 匹配，防止被 GEOSITE,google 抢占
  - DOMAIN-SUFFIX,gemini.google.com,Gemini
  - DOMAIN-SUFFIX,bard.google.com,Gemini
  - DOMAIN,generativelanguage.googleapis.com,Gemini
  - DOMAIN-SUFFIX,proactivebackend-pa.googleapis.com,Gemini
  - DOMAIN-SUFFIX,opa-pa.googleapis.com,Gemini
  - DOMAIN-SUFFIX,waa-pa.googleapis.com,Gemini
  - DOMAIN-SUFFIX,client-channel.google.com,Gemini
  - DOMAIN-SUFFIX,assistant.google.com,Gemini
  - DOMAIN-SUFFIX,ai.google.com,Gemini
  - DOMAIN-SUFFIX,aistudio.google.com,Gemini
  - DOMAIN-SUFFIX,makersuite.google.com,Gemini
  - DOMAIN-SUFFIX,googleapis.cn,Gemini
  - DOMAIN-SUFFIX,deepmind.com,Gemini
  - DOMAIN-SUFFIX,deepmind.google,Gemini
  
  # 其他 Google (搜索/地图/YouTube) 走快
  - GEOSITE,google,Gemini
  - GEOSITE,youtube,自动选择
  
  # 4. Microsoft/GitHub -> 自动选择 (求快)
  - GEOSITE,github,自动选择
  - GEOSITE,microsoft,自动选择
  
  # ♻️ 替换 RULE-SET,icloud/apple 为 Geosite,apple (内存优化)
  # (已在上方配置)

  # ⚡️ STUN 协议直连 (v1.74 最终位置)
  # 此时这里都是“未命中”上面任何代理规则的 STUN 流量。
  # 既排除了 AI/Google/TG (已走代理)，又保留了 P2P 游戏/WhatsApp (走直连)
  - PROTOCOL,STUN,DIRECT
  
  - GEOSITE,cn,国内
  - GEOSITE,geolocation-!cn,自动选择
  - GEOIP,cn,国内
  
  - MATCH,自动选择

