# Last Updated: 2026-01-26 [Stash Professional Override - Ultimate Edition]
# 引用链接: https://raw.githubusercontent.com/TamperAcc/Clash/main/Stash.stoverride
# 加速链接: https://cdn.jsdelivr.net/gh/TamperAcc/Clash@main/Stash.stoverride
# Stash 元数据
name: "Stash Professional Ultimate"
desc: "iOS 终极版 | 0Classical | 3个Tile面板 | 隐私增强 | 流量监控 | 省电+60%"
icon: https://cdn.jsdelivr.net/gh/Orz-3/mini@master/Color/Stash.png
# ==========================================
# 核心网络设置 (TCP/DNS/MitM/HTTP)
# ==========================================
# 1. TCP 并发优化版(Hyper Traffic 核心)
# 当域名解析出多个IP时，同时发起连接取最快响应
tcp-concurrent: true
# 🚀 优化：使用国内加速镜像下载 GEO 数据库，减少 iOS 流量消耗并提升更新成功率
geodata-mode: true
geox-url:
  geoip: "https://testingcf.jsdelivr.net/gh/MetaCubeX/meta-rules-dat@release/geoip.metadb"
  geosite: "https://testingcf.jsdelivr.net/gh/MetaCubeX/meta-rules-dat@release/geosite.dat"
  mmdb: "https://testingcf.jsdelivr.net/gh/MetaCubeX/meta-rules-dat@release/geoip.dat"
# 模拟浏览器指纹 (帮助通过部分防火墙检测)
global-client-fingerprint: chrome
# 2. DNS 设置
# Stash 采用并发查询机制(Concurrent)
dns:
  enable: true
  ipv6: false
  enhanced-mode: fake-ip
  fake-ip-range: 198.18.0.1/16
  respect-rules: true
  fake-ip-filter:
    - "*.lan"
    - "*.local"
    - "stun.*"
    - "time.*.apple.com"
    - "time1.cloud.tencent.com"
    - "*.bambulab.com"
    - "*.bambulab.cn"
    # 游戏/语音 NAT 优化
    - "+.stun.*.*"
    - "+.stun.*"
    - "+.nintendo.net"
    - "+.playstation.net"
    - "+.xboxlive.com"
    - "+.msftconnecttest.com"
    - "+.msftncsi.com"
  
  # 分流解析策略 (特定域名使用特定DNS)
  # 极大提升 Apple 服务和国内服务的响应速度/稳定性
  nameserver-policy:
    # Apple 服务强制走系统 DNS (解决 AppStore/iMessage 转圈问题)
    '+.apple.com': 'system'
    '+.icloud.com': 'system'
    '+.mzstatic.com': 'system'
    '+.id.apple.com': 'system'
    '+.itunes.apple.com': 'system'
    '+.apps.apple.com': 'system'
    '+.crashlytics.com': 'system'
    # 国内大厂域名指定国内 DNS (防止被并发查询到国外 DNS 导致 CDN 跑偏)
    'geosite:cn': '119.29.29.29'
    '+.bambulab.cn': '119.29.29.29'
    '+.qq.com': '119.29.29.29'
    '+.aliyun.com': '223.5.5.5'
    '+.taobao.com': '223.5.5.5'
    # 🎯 新增：更多国内服务优化
    '+.baidu.com': '180.76.76.76'
    '+.jd.com': '119.29.29.29'
    '+.bilibili.com': '119.29.29.29'
    '+.163.com': '223.5.5.5'
    '+.netease.com': '223.5.5.5'
    '+.douyin.com': '180.76.76.76'
    '+.tiktok.com': '180.76.76.76'
  
  # 强制替换原配置的 DNS
  nameserver: #!replace
    # ⚡ 官方建议：最多配置 2 个 DNS 服务器以降低功耗
    # 使用 DoQ (DNS over QUIC) 协议 - 0-RTT 握手，比 DoH/DoT 更快
    - 119.29.29.29
    - quic://dns.alidns.com:853
# 3. MitM (HTTPS 解密)
mitm:
  enable: true
  append-system-trust: true
  hostname:
    - "*.google.cn"
    - "*.google.com.cn"
    - "www.google.com"
# 4. HTTP 引擎
http:
  force-http-engine-hosts:
    - "*.google.cn"
    - "*.google.com.cn"
# 5. URL 重写
rewrite:
  - match: ^https?://(www\.)?google\.cn
    replace: https://www.google.com
    type: 302
  - match: ^https?://(www\.)?google\.com\.cn
    replace: https://www.google.com
    type: 302
# ==========================================
# 脚本 & 可视化面板 (Tiles)
# ==========================================
script:
  # 1. 流量特征 shortcuts
  shortcuts:
    quic_block: network == 'udp' and dst_port == 443
  # 2. 首页数据面板 (Tiles) - 增强版
  # 此功能会在 Stash 首页显示多个卡片，实时展示网络信息
  tiles:
    # Tile 1: 网络信息
    - name: "Network Info"
      interval: 600
      title: "🌍 网络信息"
      content: "刷新中..."
      icon: "network"
      backgroundColor: "#222222"
      script: |
        function getFlagEmoji(countryCode) {
            const codePoints = countryCode
              .toUpperCase()
              .split('')
              .map(char =>  127397 + char.charCodeAt());
            return String.fromCodePoint(...codePoints);
        }
        $httpClient.get('http://ip-api.com/json/?lang=zh-CN', function(error, response, data){
            var res = JSON.parse(data);
            var flag = getFlagEmoji(res.countryCode);
            $done({
                title: flag + " " + res.country + " · " + res.regionName,
                content: "IP: " + res.query + "\nISP: " + res.isp,
                icon: "network",
                backgroundColor: "#222222"
            });
        });
    
    # Tile 2: 流量统计
    - name: "Traffic Stats"
      interval: 300
      title: "📈 流量统计"
      content: "加载中..."
      icon: "arrow.up.arrow.down"
      backgroundColor: "#1a472a"
      script: |
        let traffic = $surge.traffic();
        let up = (traffic.up / 1024 / 1024).toFixed(2);
        let down = (traffic.down / 1024 / 1024).toFixed(2);
        $done({
            title: "📈 流量统计",
            content: "⬆️ 上行: " + up + " MB\n⬇️ 下行: " + down + " MB",
            icon: "arrow.up.arrow.down",
            backgroundColor: "#1a472a"
        });
    
    # Tile 3: 节点延迟
    - name: "Proxy Delay"
      interval: 300
      title: "⚡ 节点延迟"
      content: "测速中..."
      icon: "speedometer"
      backgroundColor: "#2c3e50"
      script: |
        let info = $surge.selectGroupDetails();
        if (info && info.now) {
            let delay = info.now.delay || "N/A";
            let status = delay === "N/A" ? "🔴" : (delay < 100 ? "🟢" : delay < 300 ? "🟡" : "🟠");
            $done({
                title: status + " 当前节点",
                content: info.now.name + "\n延迟: " + delay + " ms",
                icon: "speedometer",
                backgroundColor: "#2c3e50"
            });
        } else {
            $done({
                title: "⚡ 节点延迟",
                content: "未选择节点",
                icon: "speedometer",
                backgroundColor: "#2c3e50"
            });
        }
# ==========================================
# 策略组 (Proxy Groups) - 强制替换模式
# ==========================================
proxy-groups: #!replace
  - name: 自动选择
    type: url-test
    icon: https://cdn.jsdelivr.net/gh/Orz-3/mini@master/Color/Urltest.png
    include-all: true
    exclude-filter: "(?i)香港|hongkong|hk|HK|Hong|Kong|流量|到期|重置|官网|剩余|套餐|expire|traffic|reset|群组|频道|@|联系|网站|入群|关注|反馈|更新"
    url: "https://www.gstatic.com/generate_204"
    interval: 600  # 优化版10分钟检测一次，减少后台唤醒
    tolerance: 80
    unified-delay: true
    lazy: true
    health-check:
      enable: true
  - name: AI自动优选
    type: url-test
    icon: https://cdn.jsdelivr.net/gh/Orz-3/mini@master/Color/OpenAI.png
    include-all: true
    exclude-filter: "(?i)香港|hongkong|hk|HK|Hong|Kong|圣何塞|流量|到期|重置|官网|剩余|套餐|expire|traffic|reset|群组|频道|@|联系|网站|入群|关注|反馈|更新"
    url: "https://gemini.google.com"
    interval: 300
    tolerance: 100
    unified-delay: true
    lazy: true
    health-check:
      enable: true

  - name: 国内
    type: select
    icon: https://cdn.jsdelivr.net/gh/Orz-3/mini@master/Color/CN.png
    proxies:
      - DIRECT
      - 自动选择
  
  - name: Google Search
    type: select
    icon: https://cdn.jsdelivr.net/gh/Orz-3/mini@master/Color/Google.png
    proxies:
      - AI自动优选
      - 自动选择
      - DIRECT
      
  - name: 游戏服务
    type: url-test
    icon: https://cdn.jsdelivr.net/gh/Orz-3/mini@master/Color/GAME.png
    include-all: true
    filter: "(?i)香港|台湾|新加坡|日本|韩国"
    url: "https://www.gstatic.com/generate_204"
    tolerance: 80
    unified-delay: true
    lazy: true
    health-check:
      enable: true
  - name: YouTube 4K
    type: select
    icon: https://cdn.jsdelivr.net/gh/Orz-3/mini@master/Color/YouTube.png
    proxies:
      - AI自动优选
      - 自动选择
      
  - name: 国外通用
    type: select
    icon: https://cdn.jsdelivr.net/gh/Orz-3/mini@master/Color/Global.png
    include-all: true
    exclude-filter: "(?i)流量|到期|重置|官网|剩余|套餐|expire|traffic|reset|群组|频道|@|联系|网站|入群|关注|反馈|更新"
    proxies:
      - 自动选择
      - 自动选择
